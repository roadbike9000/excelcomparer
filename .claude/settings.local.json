{
  "permissions": {
    "allow": [
      "Bash(Select-String -Pattern \"(Passed|Failed|Total tests)\")",
      "Bash(test:*)",
      "Bash(Select-Object -Last 20)",
      "Bash(cat:*)",
      "Bash(timeout 120 dotnet test --nologo --verbosity quiet)",
      "Bash(dotnet clean)",
      "Bash(taskkill:*)",
      "Bash(timeout:*)",
      "Bash(dotnet build:*)",
      "Bash(dotnet test:*)",
      "Bash(dotnet run:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git commit:*)",
      "Bash(git init)"
    ],
    "deny": [
      "Bash(curl:*)",
      "WebFetch"
    ],
    "ask": []
  }
}
